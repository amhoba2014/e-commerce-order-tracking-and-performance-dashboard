{
	"name": "frontend",
	"image": "ecommerce_order_tracking_dev-frontend:latest",
  "workspaceMount": "source=${localWorkspaceFolder}/assets,target=/assets,type=bind,consistency=cached",
  "workspaceFolder": "/assets",
  "customizations": {
    "vscode": {
      "extensions": ["clinyong.vscode-css-modules", "bradlc.vscode-tailwindcss"],
      "settings": {
        "editor.tabSize": 2,
        "editor.detectIndentation": false
      }
    }
  }
}
